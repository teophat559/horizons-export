{
  "name": "web-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:auto": "npm run build && npm run build:watch",
    "auto:build": "node scripts/auto-build.mjs",
    "auto:build:admin": "node scripts/auto-build.mjs --admin",
    "auto:build:user": "node scripts/auto-build.mjs --user",
    "auto:build:watch": "node scripts/auto-build.mjs --watch",
    "auto:build:admin:watch": "node scripts/auto-build.mjs --admin --watch",
    "auto:build:user:watch": "node scripts/auto-build.mjs --user --watch",
    "auto:build:admin:deploy": "node scripts/auto-build.mjs --admin --deploy",
    "auto:build:user:deploy": "node scripts/auto-build.mjs --user --deploy",
    "auto:build:admin:watch:deploy": "node scripts/auto-build.mjs --admin --watch --deploy",
    "auto:build:user:watch:deploy": "node scripts/auto-build.mjs --user --watch --deploy",
    "test:auto-build": "node scripts/test-auto-build.mjs",
    "quick-test": "node scripts/quick-test.mjs",
    "test-purge": "node scripts/test-purge.mjs",
    "setup-netlify": "node scripts/setup-netlify.mjs",
    "test-deploy": "node scripts/test-deploy.mjs",
    "link-netlify": "node scripts/link-netlify.mjs",
    "fix-netlify": "node scripts/fix-netlify.mjs",
    "preflight": "node scripts/preflight.mjs",
    "clean:admin": "node scripts/clean-admin.mjs",
    "purge:legacy": "node scripts/purge-legacy-assets.mjs",
  "build:admin": "set BUILD_TARGET=admin&& npm run preflight && npm run clean:admin && vite build && node scripts/purge-legacy-assets.mjs && node scripts/verify-admin-assets.mjs && node scripts/copy-netlify-files.mjs",
    "build:admin:no-clean": "set BUILD_TARGET=admin&& set SKIP_CLEAN=true&& npm run preflight && vite build",
  "build:admin:nix": "export BUILD_TARGET=admin && npm run preflight && npm run clean:admin && vite build && node scripts/purge-legacy-assets.mjs && node scripts/verify-admin-assets.mjs && node scripts/copy-netlify-files.mjs",
  "build:admin:root": "set BUILD_TARGET=admin&& set VITE_BASE_PATH=/&& npm run preflight && npm run clean:admin && vite build && node scripts/purge-legacy-assets.mjs && node scripts/verify-admin-assets.mjs && node scripts/copy-netlify-files.mjs",
  "build:admin:root:nix": "export BUILD_TARGET=admin VITE_BASE_PATH=/ && npm run preflight && npm run clean:admin && vite build && node scripts/purge-legacy-assets.mjs && node scripts/verify-admin-assets.mjs && node scripts/copy-netlify-files.mjs",
  "build:user": "set VITE_OUT_DIR=user/dist&& npm run preflight && vite build && node scripts/copy-netlify-files.mjs",
  "build:user:nix": "export VITE_OUT_DIR=user/dist && npm run preflight && vite build && node scripts/copy-netlify-files.mjs",
    "build:admin:watch": "set BUILD_TARGET=admin&& npm run preflight && npm run clean:admin && vite build --watch",
    "build:admin:watch:nix": "export BUILD_TARGET=admin && npm run clean:admin && vite build --watch",
    "build:user:watch": "set VITE_OUT_DIR=user/dist&& npm run preflight && vite build --watch",
    "preview": "vite preview",
    "deploy:netlify:admin": "node scripts/deploy-admin-netlify.mjs",
    "deploy:netlify:user": "node scripts/deploy-user-netlify.mjs",
    "backend:install": "npm install --prefix backend",
    "backend:dev": "npm run --prefix backend dev",
    "backend:start": "npm run --prefix backend start",
    "backend:migrate": "npm run --prefix backend migrate",
    "backend:seed": "npm run backend:seed",
    "backend:setup": "npm run backend:install && npm run backend:migrate && npm run backend:seed",
    "dev:all": "concurrently -n BACKEND,FRONTEND -c magenta,cyan \"npm:backend:dev\" \"npm:dev\"",
    "dev:all:build": "concurrently -n BACKEND,FRONTEND,BUILD -c magenta,cyan,yellow \"npm:backend:dev\" \"npm:dev\" \"npm:build:watch\"",
    "dev:all:auto": "concurrently -n BACKEND,FRONTEND,AUTO -c magenta,cyan,green \"npm:backend:dev\" \"npm:dev\" \"npm:auto:build:watch\"",
    "backend:dev:detached": "start \"backend\" cmd /c \"npm run --prefix backend dev\"",
    "e2e:smoke": "npm run backend:install && npm run backend:dev:detached && npm run -s wait:health && npm --prefix backend run -s e2e:smoke",
    "e2e:vote": "npm run backend:install && npm run backend:dev:detached && npm run -s wait:health && node backend/src/scripts/e2e-vote.mjs",
    "wait:health": "node backend/src/scripts/wait-health.mjs",
    "functions:dev": "node scripts/serve-functions.mjs",
    "functions:dev:8789": "set FN_PORT=8789&& node scripts/serve-functions.mjs",
    "dev:all:morelogin": "concurrently -n BACKEND,FUNCTIONS,FRONTEND -c magenta,yellow,cyan \"npm:backend:dev\" \"npm:functions:dev\" \"npm:dev\"",
    "dev:all:morelogin:8789": "concurrently -n BACKEND,FUNCTIONS,FRONTEND -c magenta,yellow,cyan \"npm:backend:dev\" \"npm:functions:dev:8789\" \"npm:dev\"",
    "dev:all:morelogin:build": "concurrently -n BACKEND,FUNCTIONS,FRONTEND,BUILD -c magenta,yellow,cyan,green \"npm:backend:dev\" \"npm:functions:dev\" \"npm:dev\" \"npm:build:watch\"",
    "dev:all:morelogin:auto": "concurrently -n BACKEND,FUNCTIONS,FRONTEND,AUTO -c magenta,yellow,cyan,green \"npm:backend:dev\" \"npm:functions:dev\" \"npm:dev\" \"npm:auto:build:watch\"",
    "dev:prepare-ports": "scripts/kill-port.cmd 4000 && scripts/kill-port.cmd 8788 && scripts/kill-port.cmd 8789 && exit /b 0",
    "netlify:checklist": "node scripts/netlify-checklist.mjs",
    "setup:api:dev": "node scripts/setup-api.mjs --dev --memory-db true --run",
    "setup:api:prod": "node scripts/setup-api.mjs --prod",
    "git:push": "scripts/git-auto-push.cmd"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@netlify/functions": "4.2.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "puppeteer-core": "24.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.1",
    "react-toastify": "^10.0.5",
    "recharts": "^2.9.0",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "web-app": "file:"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "vite": "^4.5.14"
  }
}
